    "import numpy as np \n",
    "from scipy import stats\n",
    "array_x = np.loadtxt(\"tdata.csv\", delimiter=\",\")\n",
    "array_xs = stats.zscore(array_x)\n",
    "w, pvalue = stats.shapiro(array_xs)\n",
    "print(f'{pvalue:.3f}')"
   ]
  }
